<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="TextBrush" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#FFDDDDDD"/>
    <SolidColorBrush x:Key="ButtonTextBrush" Color="Black"/>
    <SolidColorBrush x:Key="WarningTextBrush" Color="Red"/>
    <SolidColorBrush x:Key="ListBackgroundBrush" Color="#FFF7F7F7"/>

    <SolidColorBrush x:Key="HoverBrush" Color="#FFE0E0E0"/>
    <SolidColorBrush x:Key="SelectedBrush" Color="#FFC0C0C0"/>


    <SolidColorBrush x:Key="ListItemBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="ListAlternateItemBackgroundBrush" Color="#FFF0F0F0"/>

    <SolidColorBrush x:Key="ChartBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="ChartTextBrush" Color="Black"/>
    <SolidColorBrush x:Key="CandleIncreasingBrush" Color="Green"/>
    <SolidColorBrush x:Key="CandleDecreasingBrush" Color="Red"/>

    <SolidColorBrush x:Key="ComboBoxBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="ComboBoxForegroundBrush" Color="Black"/>
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#FFAAAAAA"/>
    <SolidColorBrush x:Key="ComboBoxHoverBackgroundBrush" Color="#FFDCEBF9"/>
    <SolidColorBrush x:Key="ComboBoxFocusedBorderBrush" Color="#FF569DE5"/>
</ResourceDictionary>
