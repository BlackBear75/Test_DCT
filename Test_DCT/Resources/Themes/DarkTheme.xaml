<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundBrush" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="TextBrush" Color="White"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#FF333333"/>
    <SolidColorBrush x:Key="ButtonTextBrush" Color="White"/>
    <SolidColorBrush x:Key="WarningTextBrush" Color="#FFFF5555"/>
    <SolidColorBrush x:Key="ListBackgroundBrush" Color="#FF2A2A2A"/>

    <SolidColorBrush x:Key="HoverBackgroundBrush" Color="#FF444444" />
    <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="#FF666666" />
    <SolidColorBrush x:Key="SelectedForegroundBrush" Color="White" />
    
    
</ResourceDictionary>
